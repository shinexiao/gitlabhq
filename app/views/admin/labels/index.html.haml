- page_title "Labels"

%div
  = link_to new_admin_label_path, class: "pull-right btn btn-nr btn-new" do
    New label
  %h3.page-title
    Labels
%hr

.labels
  - if @labels.present?
    %ul.bordered-list.manage-labels-list
      = render @labels
    = paginate @labels, theme: 'gitlab'
  - else
    .light-well
      .nothing-here-block There are no labels yet

